			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077010">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:narrower><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077001">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">ultimate</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Team game. Played outdoors on soccer-size field with 7 players per team and indoors with 5 players per team. Games are played to a preset number of goals. Goals are scored by passing the disc to a team-member catching it inside the goal area. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077002">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">guts</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Team game where two teams of 5 players each face each other. Teams take turns to throw the disc towards the other team. Points are scored if the opposing team does not catch the disc. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077003">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">overall</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Not an event in itself but in most competitions with individual events the winner of the competition will be awarded to the player with the highest combined score in the events. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077004">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">distance</skos:prefLabel>
		<skos:definition xml:lang="en-GB">The object is to throw the disc as far as possible. Each player get five throws per turn. Throws are made downwind. World record is over 200 meters. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077005">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">discathon</skos:prefLabel>
		<skos:definition xml:lang="en-GB">The object is to throw and run a park course of about 1 kilometre. Four players compete in each heat. They have two discs each. They may also carry a third disc in reserve. Where one thrown disc stops the second disc must be thrown. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077006">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">DDC</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Abbreviation for Double Disc Court. Two players per team in a square 13x13 metre. The teams start with one disc per team and throw at each other. Discs are thrown back and forth until one or both hit the ground. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077007">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">SCF</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Abbreviation for Self Caught Flights. Divided into two sub events called MTA (Maximum Time Aloft) and TRC (Throw Run Catch). In both sub events the player throws a disc and then runs to catch it with one hand before it hits the ground. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077008">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">freestyle</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A judged event where groups of two or three players perform as many tricks as possible. In competitions the judges can use either a ranking system or set scores on presentation, execution and difficulty. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077009">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">accuracy</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Each player has 28 throws from various distances and angles to throw the disc through a goal 1.5 x 1.5 meters. World record is 25 of 28 possible. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077010">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">disc golf</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Much like golf but played with discs instead of balls and clubs. Instead of a hole in the ground there is a metal basket with light metal chains to throw the disc at. Players often carry discs specialized for long throws, right or left bends. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15077000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15078000">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">floorball</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Played indoors in a court the size of a basketball court. 6 players per team, of which one is a who plays in a kneeling position without a stick like in ice hockey. The other players use plastic clubs and a light plastic ball to pass and shoot goals. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15000000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15079000">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">casting</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Using fishing equipment to hit a target and score points </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15000000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15080000">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">tug-of-war</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Two teams pulling against each other on a rope </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/subjectcode/"/><!--Broader Relationships--><skos:broader>
			<rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15000000">
				<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
			</rdf:Description>
		</skos:broader><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/subjectcode/15081000">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">croquette</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Using mallets to hit a ball through hoops </skos:definition>
